{
	"name": "remotion-template-still",
	"version": "1.0.0",
	"description": "A template for designing still images with dynamic data with built-in server for deploying to the cloud.",
	"scripts": {
		"start": "tsc && node dist/server/index.js",
		"dev": "remotion preview src/index.tsx",
		"render": "remotion still src/index.tsx SplitScreenLogo out/SplitScreenLogo.png",
		"renderGif": "remotion render src/index.tsx Mattm9yMeta out/Mattm9yMeta.gif --codec=gif",
		"upgrade": "remotion upgrade",
		"server": "ts-node src/server/index.tsx",
		"test": "eslint src --ext ts,tsx,js,jsx && tsc",
		"publish": "node publish.mjs"
	},
	"repository": {},
	"license": "UNLICENSED",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.25.0",
		"@fontsource/archivo-black": "^4.5.9",
		"@remotion/bundler": "4.1.0-alpha3",
		"@remotion/cli": "4.1.0-alpha3",
		"@remotion/cloudrun": "4.1.0-alpha3",
		"@remotion/lambda": "4.1.0-alpha2",
		"@remotion/eslint-config": "4.1.0-alpha3",
		"@remotion/google-fonts": "4.1.0-alpha3",
		"@remotion/renderer": "4.1.0-alpha3",
		"@types/express": "^4.17.13",
		"@types/express-rate-limit": "^5.1.3",
		"@types/react": "^18.0.6",
		"@types/web": "^0.0.61",
		"autoprefixer": "^10.4.7",
		"css-loader": "^6.7.1",
		"dotenv": "^10.0.0",
		"eslint": "^8.14.0",
		"express": "^4.17.1",
		"express-rate-limit": "^5.3.0",
		"p-limit": "^3.1.0",
		"postcss": "^8.4.14",
		"postcss-loader": "^7.0.1",
		"postcss-preset-env": "^7.7.2",
		"prettier": "^2.6.2",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"remotion": "4.1.0-alpha3",
		"style-loader": "^3.3.1",
		"tailwindcss": "^3.3.2",
		"ts-node": "^9.1.1",
		"typescript": "^4.6.3",
		"webpack": "^5.0.0"
	}
}